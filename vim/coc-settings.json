{
  "codeLens.enable": true,
  "elixir.pathToElixirLS": "~/Projects/source/elixir-ls/release/language_server.sh",
  "git.addGBlameToBufferVar": true,
  "languageserver": {
    "elixirLS": {
      "command": "~/Projects/source/elixir-ls/release/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        "stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    },
    "ocaml": {
      "command": "~/Projects/utils/rls-macos/reason-language-server",
      "args": ["--stdio"],
      "filetypes": ["ocaml", "reason"]
    }
  },
  "signature.target": "echo"
}
